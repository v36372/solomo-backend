.col-md-6.col-md-offset-3
  .progress.progress-striped.actived
    .progress-bar.progress-bar-primary{role: "progressbar", "aria-valuenow" => "20", "aria-valuemin" => "0", "aria-valuemax" => "100", style: 'width: 100%'}
  #store_registration
    = form_for current_user, url: stores_registrations_path, method: :patch do |f|
      = render "stores/registrations/form", f: f
